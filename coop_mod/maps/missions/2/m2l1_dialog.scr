	//entActor.alias( "liftFixedHarry3", "localization/sound/dialog/m2l1/harry_whimper.mp3", "stop" );	//Ohhh no!!  no!!  Not again!! *whimpering*
	//entActor.alias( "liftFixedHarry4", "localization/sound/dialog/m2l1/harry_whyme.mp3" );				//*whimpering*…..why me????
	
