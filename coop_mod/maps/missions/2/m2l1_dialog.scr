//-------------------------------------------
// initDialog_Porter
// setup +use dialog for Porter, the maintenance man
//-------------------------------------------
void initDialog_Porter( entity entActor )
{
	//--- dialog and watch modes
	entActor.setdialogmode( "normal" );
	entActor.settalkwatchmode ( "headwatchonly" );
	
	//--- alias and registering
	//--- before finding the broken lift
	entActor.alias( "liftNormalPorter1", "localization/sound/dialog/m2l1/porter_newguy.mp3", "stop" );			//I'm the repair tech here at Starfleet Academy.  My name is Porter.
	entActor.alias( "liftNormalPorter2", "localization/sound/dialog/m2l1/porter_beenwhile.mp3", "stop" );		//I've been here longer then most of the teachers.  I've forgotten more about engineering then you'll ever know.
	entActor.alias( "liftNormalPorter3", "localization/sound/dialog/m2l1/porter_yupgood.mp3", "stop" );			//If you need anything fixed, just find me.  Nice meeting you, lieutenant.
	entActor.alias( "liftNormalPorter4", "localization/sound/dialog/m2l1/porter_likesaid.mp3" );				//Like I said, if you need anything taken care of, just let me know.
	
	//--- after finding the lift and en-route to fix it
	entActor.alias( "liftGotPorterPorter1", "localization/sound/dialog/m2l1/porter_blasted.mp3", "stop" );		//Damn turbolifts! They’re almost as bad as transporters!
	entActor.alias( "liftGotPorterPorter2", "localization/sound/dialog/m2l1/porter_flakey.mp3", "stop" );		//Oh yeah. Transporters are unsafe!  I won't be caught dead in one.
	entActor.alias( "liftGotPorterPorter3", "localization/sound/dialog/m2l1/porter_onetime.mp3", "stop" );		//You never know if you'll end up with your head on backwards when the transporter finally spits you out.
	entActor.alias( "liftGotPorterPorter4", "localization/sound/dialog/m2l1/porter_darnstup.mp3", "stop" );		//Nope. Don’t trust transporters. Or communicators.
	entActor.alias( "liftGotPorterPorter5", "localization/sound/dialog/m2l1/porter_somefood.mp3" );				//I don’t trust anything I don’t fix with my own hands.
	//entActor.alias( "liftGotPorterPorter6", "localization/sound/dialog/m2l1/porter_turnip.mp3", "stop" );		//You ever seen a  turnip crossed with a radish???  Believe me son….you don't want to!!
	//entActor.alias( "liftGotPorterPorter7", "localization/sound/dialog/m2l1/porter_veggies.mp3" );				//Ohhhh the veggies……musn't think about it!!
	
	//--- waiting for the player to return with the phase inverter
	entActor.alias( "liftWaitingPorter1", "localization/sound/dialog/m2l1/porter_gottasay.mp3", "stop" );		//O, I better stay here and make sure nobody tries to use this lift!
	entActor.alias( "liftWaitingPorter2", "localization/sound/dialog/m2l1/porter_officeover.mp3", "stop" );		//My office is over there in the main building. You should have no trouble finding it.
	entActor.alias( "liftWaitingPorter3", "localization/sound/dialog/m2l1/porter_snag.mp3", "stop" );			//If you hit a snag, find Harry. He should be able to help you.
	entActor.alias( "liftWaitingPorter4", "localization/sound/dialog/m2l1/porter_goonnow.mp3" );				//Go on now. I don't have all day.
	//entActor.alias( "liftWaitingPorter5", "localization/sound/dialog/m2l1/porter_tarnat.mp3", "stop" );			//Look I really need you to find my assistant.  Can you hurry up?
	//entActor.alias( "liftWaitingPorter6", "localization/sound/dialog/m2l1/porter_outhtere.mp3", "stop" );			//You know..I'm still HERE!!!  I need to be OUT THERE!  And I'm HERE!!  Ya..ya see!!  I'm HERE now!!  Do you understand that!!  And I'm only here because I'm waitin' on your slow butt!!
	//entActor.alias( "liftWaitingPorter7", "localization/sound/dialog/m2l1/porter_swear.mp3" );					//I swear, I regret the day they took my phaser!!!

	//--- after the lift is fixed
	entActor.alias( "liftFixedPorter1", "localization/sound/dialog/m2l1/porter_bigprob.mp3", "stop" );			//It's fixed.  No big problem. Just a minor malfunction in that pulse phase inverter.
	//entActor.alias( "liftFixedPorter2", "localization/sound/dialog/m2l1/porter_darnfools.mp3", "stop" );		//Darned fools they have makin' some of this stuff….makes you wonder if they speak English!!
	entActor.alias( "liftFixedPorter2", "localization/sound/dialog/m2l1/porter_harrym.mp3", "stop" );			//Well, I gotta go lock Harry in the maintenance office. I'll see ya ‘round.
	entActor.alias( "liftFixedPorter3", "localization/sound/dialog/m2l1/porter_likesaid2.mp3" );				//Like I said, if ya need anything taken care of, just drop by.
	
	entActor.alias( "greetingPorter1", "localization/sound/dialog/ent_m1/munro_hello.mp3" );		//Hello
	entActor.alias( "greetingPorter2", "localization/sound/dialog/ent_m1/munro_porter.mp3" );		//Porter.
	entActor.alias( "greetingPorter3", "localization/sound/dialog/ent_m1/munro_hporter.mp3" );		//Hey Porter.

	entActor.dialog( "liftNormalPorter", "depends", "level.liftNormal" );
	entActor.dialog( "liftGotPorterPorter", "depends", "level.liftGotPorter" );
	entActor.dialog( "liftWaitingPorter", "depends", "level.liftWaiting" );
	entActor.dialog( "liftFixedPorter", "depends", "level.liftFixed" );
	entActor.dialog( "greetingPorter", "greetingDialog" );
}


