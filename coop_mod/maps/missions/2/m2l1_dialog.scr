//-------------------------------------------
// initDialog_OfficialMale
// setup generic dialog for an official male
//-------------------------------------------
void initDialog_OfficialMale( entity entActor, float boolDoConversationalAnims, string strHeadCommand )
{
	float intChoice;
	float intActorChoice;
	float intCounterCurrent;
	
	string strAliasBase;
	string strAliasGeneral1;
	string strAliasGeneral2;
	string strAliasGeneral_Base;
	string strAliasGreeting1;
	string strAliasGreeting2;
	string strAliasGreeting_Base;
	
	//--- create unique alias'
	intCounterCurrent = intCounter;
	intCounter ++;
	strAliasBase = "OfficialMale" + intCounterCurrent;
	strAliasGeneral1 = strAliasBase + "_general1";
	strAliasGeneral2 = strAliasBase + "_general2";
	strAliasGeneral_Base = strAliasBase + "_general";
	strAliasGreeting1 = strAliasBase + "_greeting1";
	strAliasGreeting2 = strAliasBase + "_greeting2";
	strAliasGreeting_Base = strAliasBase + "_greeting";
	
	//--- pick a dialog choice
	intChoice = randomint( 2 );
	
	//--- pick an actor choice
	intActorChoice = randomint( 3 );
	
	//--- if the choice picked was the previous one used, pick again until it's different
	while( (intChoice == intChoicePrevious_OfficialMale) && (intActorChoice == intActorChoicePrevious_OfficialMale) )
	{
		intChoice = randomint( 2 );
		intActorChoice = randomint( 3 );
	}
	
	//--- store the setting for checking on the next person
	intChoicePrevious_OfficialMale = intChoice;
	intActorChoicePrevious_OfficialMale = intActorChoice;

	//print( "----------------------------------------------------------------------------------------------\n" );
	//print( "DIALOG MALE OFFICIAL: actor: " + entActor.getrawtargetname() + " : basename: " + strAliasBase +"\n" );
	//print( "DIALOG MALE OFFICIAL: actor choice: " + intActorChoice + " : dialog choice: " + intChoice + "\n" );
	//print( "----------------------------------------------------------------------------------------------\n" );
	
	//--- start setting dialog
	entActor.setdialogmode( "normal" );
	entActor.settalkwatchmode ( strHeadCommand, boolDoConversationalAnims );
	
	if( intActorChoice == 0 )  //--- actor choice 1
	{
    	if( intChoice == 0 )	//--- only alias the 'I'm busy' line
    	{
    		entActor.alias( strAliasGeneral1 , "localization/sound/dialog/m2l1/officialm1_chat.mp3" ); 	//I wish I could chat right now, Lieutenant; perhaps later.
    		//print( "DIALOG MALE OFFICIAL: actor0: " + strAliasBase + " : general1 alias - can't talk now: " + strAliasGeneral1 + "\n" );
    	}
    	else
    	{
    		//--- alias the common lines
    		entActor.alias( strAliasGeneral1 , "localization/sound/dialog/m2l1/officialm1_glad.mp3", "stop" ); 	//I'm glad Voyager made it back safely. What an amazing high point to a career in Starfleet.
    		//print( "DIALOG MALE OFFICIAL: actor0: " + strAliasBase + " : general1 alias: " + strAliasGeneral1 + "\n" );
    		entActor.alias( strAliasGeneral2 , "localization/sound/dialog/m2l1/officialm1_good.mp3" ); 			//Good day to you Lieutenant Munro.
    		//print( "DIALOG MALE OFFICIAL: actor0: " + strAliasBase + " : general2 alias: " + strAliasGeneral2 + "\n" );
    	}
    }
    else if( intActorChoice == 1 )	//--- actor choice 2
    {
    	if( intChoice == 0 )	//--- only alias the 'I'm busy' line
    	{
    		entActor.alias( strAliasGeneral1 , "localization/sound/dialog/m2l1/officialm2_come.mp3" ); 	//If you come back another time, I should be able to spare a few moments professor.
    		//print( "DIALOG MALE OFFICIAL: actor1: " + strAliasBase + " : general1 alias - can't talk now: " + strAliasGeneral1 + "\n" );
    	}
    	else
    	{
    		//--- alias the common lines
    		entActor.alias( strAliasGeneral1 , "localization/sound/dialog/m2l1/officialm2_need.mp3", "stop" ); 			//Is there anything you need?
    		//print( "DIALOG MALE OFFICIAL: actor1: " + strAliasBase + " : general1 alias: " + strAliasGeneral1 + "\n" );
    		entActor.alias( strAliasGeneral2 , "localization/sound/dialog/m2l1/officialm2_hello.mp3" ); 				//Hello professor
    		//print( "DIALOG MALE OFFICIAL: actor1: " + strAliasBase + " : general2 alias: " + strAliasGeneral2 + "\n" );
    	}
    }
    else	//--- actor choice 3
    {
		//--- alias the common lines
		entActor.alias( strAliasGeneral1 , "localization/sound/dialog/m2l1/malead1_onepiece.mp3", "stop" ); 	//It's good to have Voyager back safe and in one piece
		//print( "DIALOG MALE OFFICIAL: actor2: " + strAliasBase + " : general1 alias: " + strAliasGeneral1 + "\n" );
		entActor.alias( strAliasGeneral2 , "localization/sound/dialog/m2l1/malead1_welcome.mp3" ); 				//Welcome to Starfleet Academy Lieutenant Munro
		//print( "DIALOG MALE OFFICIAL: actor2: " + strAliasBase + " : general2 alias: " + strAliasGeneral2 + "\n" );
    }
    	
	entActor.alias( strAliasGreeting1 , "localization/sound/dialog/ent_m2/munro_sir.mp3" );	//sir
	
	//print( "============== REGISTER DIALOG ====================\n" );
	
	//-------------------------------------------------------------------
	//--- register all the alias'
	entActor.dialog( strAliasGeneral_Base );
	//print( "DIALOG MALE OFFICIAL: actor: " + strAliasBase + " : general base alias: " + strAliasGeneral_Base + "\n" );
	entActor.dialog( strAliasGreeting_Base, "greetingDialog" );

	//print( "--------------------------------- END CHARACTER ----------------------------------\n" );
	//print( "----------------------------------------------------------------------------------\n" );
}


