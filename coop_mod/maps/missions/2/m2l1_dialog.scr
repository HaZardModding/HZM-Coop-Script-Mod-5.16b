//--------------------------------------------------------------------------------------
// Dialog Script
// 	Contains all dialog setup code for one specific BSP.  That map's script should
//	#include this file and call the function "initDialog" somewhere in it's main function.
//--------------------------------------------------------------------------------------
void initDialog()
{
	//print( "SETTING UP DIALOG!!!\n" );
	
	//----------------------------------------------------------------------------------
	//--- MAIN BUILDING
	//----------------------------------------------------------------------------------

	//-----------------------------------------
	//--- stemmons
	//--- dialog and watch modes
	$stemmons.setdialogmode( "normal" );
	$stemmons.settalkwatchmode ( "ignore", 0 );
	
	//--- alias and registering
	$stemmons.alias( "generalStemmonsDialog1" , "localization/sound/dialog/m2l1/stem_dont.mp3", "stop" );	//The subject of your reassignment is closed, Lieutenant.
	$stemmons.alias( "generalStemmonsDialog2" , "localization/sound/dialog/m2l1/stem_out.mp3", "stop" );	//There is nothing to be gained by petitioning my office. No one sees a need for a Hazard Team.
	$stemmons.alias( "generalStemmonsDialog3" , "localization/sound/dialog/m2l1/stem_prec2.mp3" );			//I mean precisely what I say, Lieutenant. Now, if you don’t mind, some of us have jobs to do. 
	
	$stemmons.dialog( "generalStemmonsDialog" );

	//--- receptionist
	initDialog_Receptionist( $mainBuilding_ReceptionistFemale );

	//--- officials
	initDialog_OfficialMale( $mainBuilding_OfficialMale1, TRUE, "headwatchonly" );
	initDialog_OfficialMale( $mainBuilding_OfficialMale2, TRUE, "headwatchonly" );
	initDialog_OfficialFemale( $mainBuilding_OfficialFemale1, TRUE, "headwatchonly" );
	initDialog_OfficialFemale( $mainBuilding_OfficialFemale2, TRUE, "headwatchonly" );
	initDialog_OfficialFemale( $mainBuilding_OfficialFemale3, TRUE, "headwatchonly" );

	//--- teachers
	initDialog_TeacherMale( $mainBuilding_TeacherMale1, TRUE, "headwatchonly" );
	initDialog_TeacherMale( $mainBuilding_TeacherMale2, FALSE, "turnto" );
	initDialog_TeacherMale( $mainBuilding_TeacherMale4, FALSE, "ignore" );

	//--- cadets
	initDialog_CadetMale( $mainBuilding_CadetMale1, FALSE, "headwatchonly" );
	initDialog_CadetMale( $mainBuilding_CadetMale2, FALSE, "headwatchonly" );
	initDialog_CadetMale( $mainBuilding_CadetMale3, FALSE, "ignore" );
	initDialog_CadetMale( $mainBuilding_CadetMale4, FALSE, "headwatchonly" );
	initDialog_CadetMale( $mainBuilding_CadetMale5, TRUE, "headwatchonly" );
	initDialog_CadetFemale( $mainBuilding_CadetFemale1, TRUE, "headwatchonly" );
	initDialog_CadetFemale( $mainBuilding_CadetFemale2, FALSE, "headwatchonly" );
	initDialog_CadetFemale( $mainBuilding_CadetFemale3, FALSE, "headwatchonly" );
	initDialog_CadetFemale( $mainBuilding_CadetFemale4, FALSE, "turnto" );
	initDialog_CadetFemale( $mainBuilding_CadetFemale5, FALSE, "ignore" );
	initDialog_CadetFemale( $mainBuilding_CadetFemale6, TRUE, "turnto" );

	//----------------------------------------------------------------------------------
	//--- COURTYARD
	//----------------------------------------------------------------------------------

	//--- porter the maintenance man
	//initDialog_Porter( $porter );

	//--- harry, porter's assistant
	initDialog_Harry( $harry );

	//--- officials
	initDialog_OfficialMale( $mainCourtyard_OfficialMale1, TRUE, "headwatchonly" );
	initDialog_OfficialMale( $mainCourtyard_OfficialMale2, TRUE, "turnto" );
	//initDialog_OfficialMale( $mainCourtyard_OfficialMale3, FALSE, "turnto" );
	initDialog_OfficialMale( $porter, TRUE, "headwatchonly" );
	initDialog_OfficialFemale( $mainCourtyard_OfficialFemale1, TRUE, "headwatchonly" );
	initDialog_OfficialFemale( $mainCourtyard_OfficialFemale2, TRUE, "headwatchonly" );
	
	//--- teachers
	initDialog_TeacherMale( $mainCourtyard_TeacherMale1, FALSE, "ignore" );
	initDialog_TeacherMale( $mainCourtyard_TeacherMale2, TRUE, "headwatchonly" );
	initDialog_TeacherFemale( $mainCourtyard_TeacherFemale1, TRUE, "headwatchonly" );
	initDialog_TeacherFemale( $mainCourtyard_TeacherFemale2, TRUE, "headwatchonly" );
	initDialog_TeacherFemale( $mainCourtyard_TeacherFemale3, FALSE, "headwatchonly" );
	initDialog_TeacherFemale( $mainCourtyard_TeacherFemale4, FALSE, "headwatchonly" );

	//--- cadets
	//initDialog_CadetMale( $mainCourtyard_CadetMale1, TRUE, "turnto" );
	initDialog_CadetMale( $mainCourtyard_CadetMale2, TRUE, "headwatchonly" );
	initDialog_CadetMale( $mainCourtyard_CadetMale3, FALSE, "ignore" );
	initDialog_CadetMale( $mainCourtyard_CadetMale4, FALSE, "headwatchonly" );
	initDialog_CadetMale( $mainCourtyard_CadetMale5, FALSE, "ignore" );
	initDialog_CadetMale( $mainCourtyard_CadetMale6, FALSE, "headwatchonly" );
